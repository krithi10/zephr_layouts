<a class="ear" data-side="{{ear-side}}" href="{{subscribe-url}}" aria-label="{{ada-content}}" style="background:url({{image-url}}) 
center no-repeat;background-size:100%;display:inline-block;border-radius:3px;height:{{image-height}}px;width:{{image-width}}px;color:transparent">{{ada-content}}</a>

<script>
(() => {
console.log("-----------loading Ears---------------")
	let earSide = document.querySelector("[data-side='{{ear-side}}']");
	if(earSide.dataset.side === "left"){
            document.querySelector("[data-reg-role='ear_left']").appendChild(earSide);
  	} 
	if(earSide.dataset.side === "right"){
            document.querySelector("[data-reg-role='ear_right']").appendChild(earSide);
  	}
})();
</script>

